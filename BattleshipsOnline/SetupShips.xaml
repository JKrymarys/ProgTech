                <RadioButton Content="Vertical" GroupName="directionButtons" Tag="Vertical" FontSize="18" Checked="DirectionButtonVerical_Checked"/>
            </StackPanel>
            
            <Path Stroke="Black" StrokeThickness="2" Fill="#FFA2A2F1" Canvas.Left="47.1" Canvas.Top="7.4" Height="28.8" Stretch="Fill" Width="56.8"
            		x:Name="destroyer" MouseLeftButtonDown="ship_MouseLeftButtonDown">
                <Path.Data>
                    <GeometryGroup>
                        <EllipseGeometry Center="110 10" RadiusX="100" RadiusY="90" />
                        <LineGeometry StartPoint="110 110" EndPoint="110 -90" />
                    </GeometryGroup>
                </Path.Data>
            </Path>
            <Path Stroke="Black" StrokeThickness="2" Fill="#FFA2A2F1" Canvas.Left="138" Canvas.Top="8.8" Height="27.2" Stretch="Fill" Width="74.4"
